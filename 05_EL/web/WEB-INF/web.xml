<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <context-param>
    <param-name>namespace</param-name>
    <param-value>超哥无敌</param-value>
    </context-param>
    <context-param>
        <param-name>name</param-name>
        <param-value>丹姐无敌</param-value>
    </context-param>
    <servlet>
        <servlet-name>uploading</servlet-name>
        <servlet-class>com.bdqn.servlet.uploading</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>session</servlet-name>
        <servlet-class>com.bdqn.servlet.session</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>xia</servlet-name>
        <servlet-class>com.bdqn.servlet.xia</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>dbut</servlet-name>
        <servlet-class>com.bdqn.servlet.dbut</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>top</servlet-name>
        <servlet-class>com.bdqn.servlet.top</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>top</servlet-name>
        <url-pattern>/t</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>dbut</servlet-name>
        <url-pattern>/d</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>xia</servlet-name>
        <url-pattern>/x</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>session</servlet-name>
        <url-pattern>/s</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>uploading</servlet-name>
        <url-pattern>/up</url-pattern>
    </servlet-mapping>
</web-app>