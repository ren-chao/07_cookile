<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>sele</servlet-name>
        <servlet-class>cn.bdqn.servlet.sele</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>sele</servlet-name>
        <url-pattern>/se</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ser</servlet-name>
        <servlet-class>cn.bdqn.dao.ser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addbook</servlet-name>
        <servlet-class>cn.bdqn.servlet.addbook</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ser</servlet-name>
        <url-pattern>/le</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addbook</servlet-name>
        <url-pattern>/add</url-pattern>
    </servlet-mapping>
</web-app>